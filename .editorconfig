####
## This file helps to maintain a consistent coding style
## between different envirorments.
## More info: editorconfig.org
##

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
